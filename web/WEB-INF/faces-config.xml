<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <navigation-case>
            <from-outcome>bienvenido</from-outcome>
            <to-view-id>/bienvenido.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>cruds</from-outcome>
            <to-view-id>/cruds.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>super</from-outcome>
            <to-view-id>/super.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
</faces-config>
